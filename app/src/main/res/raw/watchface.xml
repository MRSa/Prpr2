<WatchFace width="300" height="300" shape="CIRCLE">
    <Scene>
        <DigitalClock x="0" y="0" width="300" height="300">
            <!-- Interactive mode version -->
            <TimeText format="hh:mm" hourFormat="SYNC_TO_DEVICE" align="CENTER" x="75" y="200" width="150" height="100" alpha="255">
                <Variant mode="AMBIENT" target="alpha" value="0"/>
                <Font family="SYNC_TO_DEVICE" size="55" weight="NORMAL" slant="NORMAL" color="#ee5191ff"/>
            </TimeText>

            <!-- Ambient mode version -->
            <TimeText format="hh:mm" hourFormat="SYNC_TO_DEVICE" align="CENTER" x="75" y="200" width="150" height="100" alpha="0">
                <Variant mode="AMBIENT" target="alpha" value="255"/>
                <Font family="SYNC_TO_DEVICE" size="55" weight="THIN" slant="NORMAL" color="#ee5191ff"/>
            </TimeText>
        </DigitalClock>
        <Group x="0" y="0" width="300" height="300" name="background_image">
            <PartImage x="0" y="0" width="300" height="300">
                <Variant mode="AMBIENT" target="alpha" value="0"/>
                <Image resource="@drawable/ao1" />
            </PartImage>
        </Group>

    </Scene>
    <Metadata key="PREVIEW_TIME" value="12:34:56"/>
    <Metadata key="CLOCK_TYPE" value="DIGITAL" />

</WatchFace>
